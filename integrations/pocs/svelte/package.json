{
  "name": "agent-neo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/agent-neo.umd.cjs",
  "module": "./dist/agent-neo.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/agent-neo.js",
