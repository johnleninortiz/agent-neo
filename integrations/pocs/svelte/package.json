  "sideEffects": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "svelte-preprocess": "^6.0.2",
    "svelte": "^4.2.18",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  }
}
